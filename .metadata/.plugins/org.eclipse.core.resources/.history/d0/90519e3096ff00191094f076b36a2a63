package javainterviewquestions;

public class missinfnumberinarray {

	public static void main(String[] args) {
		int a[]= {1,2,3,4,5,6,7,9};
		int sum=0;
		for(int i=0;i<a.length;i++) {
			sum=sum+a[i];	
		}
		System.out.println(sum);
		int sum1=0;
		for(int k=1;k<=9;k++) {
			sum1=sum1+k;
		}
		System.out.println(sum1);
		
	}
}
